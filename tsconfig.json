{
  "compilerOptions": {
    "lib": ["es2018", "dom"],
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES5",
    "baseUrl": ".",
    "downlevelIteration": true,
    "paths": {
      "*": [
        "modules/*",
        "node_modules/*"
      ]
    }
  }
}
